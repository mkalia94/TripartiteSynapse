#!/bin/bash
declare -A ECSsizes
ECSsizes=( ["-s"]="SmallECS"
#           ["-m"]="MedECS"
            ["-b"]="BigECS")

declare -A Experiments
Experiments=(   ["Bif1"]=" --perc 0.1 --tstart 20 --tend 140 --tfinal 200"
		["Bif2"]=" --perc 0.2 --tstart 20 --tend 140 --tfinal 200"
		["Bif3"]=" --perc 0.3 --tstart 20 --tend 140 --tfinal 200"
		["Bif4"]=" --perc 0.4 --tstart 20 --tend 140 --tfinal 200"
		["Bif5"]=" --perc 0.5 --tstart 20 --tend 140 --tfinal 200"
		["Bif6"]=" --perc 0.6 --tstart 20 --tend 140 --tfinal 200"
		["Bif7"]=" --perc 0.7 --tstart 20 --tend 140 --tfinal 200"
		["Bif8"]=" --perc 0.8 --tstart 20 --tend 140 --tfinal 200"
		["Bif9"]=" --perc 0.9 --tstart 20 --tend 140 --tfinal 200"
		#Pscale1 is just Bif3
		["Pscale2"]="--perc 0.3 --tstart 20 --tend 140 --tfinal 200 --pumpScaleNeuron 1.6 --pumpScaleAst 1.6"
		["Pscale3"]="--perc 0.3 --tstart 20 --tend 140 --tfinal 200 --pumpScaleNeuron 1.8 --pumpScaleAst 1.8"
		["Pscale4"]="--perc 0.3 --tstart 20 --tend 140 --tfinal 200 --pumpScaleNeuron 2 --pumpScaleAst 2"
		["Pscale5"]="--perc 0.3 --tstart 20 --tend 140 --tfinal 200 --pumpScaleNeuron 2.2 --pumpScaleAst 2.2"
		["Pscale6"]="--perc 0.3 --tstart 20 --tend 140 --tfinal 200 --pumpScaleNeuron 2.4 --pumpScaleAst 2.4"
		["Pscale7"]="--perc 0.3 --tstart 20 --tend 140 --tfinal 200 --pumpScaleNeuron 2.6 --pumpScaleAst 2.6")
		# ["pED_IClGblock"]="--tstart 20 --tend 50 --perc 0.5 --alphae0 0.7 --tfinal 80 --block {\"IClG\":[20,50]}"
	        # ["pED_INaGblock"]="--tstart 20 --tend 50 --perc 0.5 --alphae0 0.7 --block {\"INaG\":[20,50]} --tfinal 80"
            # ["pED_INCXiblock"]="--tstart 20 --tend 50 --perc 0.5 --alphae0 0.7 --block {\"INCXi\":[20,50]} --tfinal 80 "
            # ["pED_INCXgGblock"]="--tstart 20 --tend 50 --perc 0.5 --alphae0 0.7 --block {\"INCXg\":[20,50]} --tfinal 80"
            # ["pED_GLTgblock"]="--tstart 20 --tend 50 --perc 0.5 --alphae0 0.7 --block {\"fGLTg\":[20,50]} --tfinal 80"
	     # ["pED_GLTiblock"]="--tstart 20 --tend 50 --perc 0.5 --alphae0 0.7 --block {\"fGLTi\":[20,50]} --tfinal 80")
	      
for name_ in "${!Experiments[@]}"; do
  for size_ in "${!ECSsizes[@]}"; do
    python set_up.py \
    --name "${ECSsizes[$size_]}" \
    $size_ \
    ${Experiments[$name_]} \
    --solve \
    --saveloc "Exp2310/$name_/${ECSsizes[$size_]}" \
    --plotall
  done; 
done
