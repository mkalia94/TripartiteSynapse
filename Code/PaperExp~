#!/bin/bash
declare -A Experiments
Experiments=(
	["Calibration_Fig1"]="--tstart 10 --perc 0.4 --alphae0 0.7 --casename \"Case1\" --case1 {\"tend\":13,\"tfinal\":20} --case2 {\"tend\":20,\"tfinal\":30}"
	["PathPhys_Fig2"]="--tstart 200 --tend 500 --tfinal 3 --excite [1,2] -s --casename \"Stim_10_5_95e_2\" --case1 {\"perc\":0.4} --case2 {\"astblock\":[1,2]}"
	)
for name_ in "${!Experiments[@]}"; do
    python set_up.py \
    ${Experiments[$name_]} \
    --solve \
    --saveloc "PaperExp/$name_"
done;