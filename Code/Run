#!/bin/bash
python set_up.py --solve --saveloc 'Finalfigures/ShortED' --readdata ''  --alphae0 0.8 --t0 15 --tstart 20 --tend 25 --tfinal 35  --perc 0.5 --gltScale 0.5 --gltScaleAst 2 --pumpScaleNeuron 1.6 --pumpScaleAst 1.6  --plot '{"E$_m$ (mV)":{"plot":["ax1Vi","ax2Vg"]},"[Na$^+$] (mM)":{"plot":["ax1NaCi","ax1NaCg","ax2NaCe"]},"[K$^+$] (mM)":{"plot":["ax1KCi","ax1KCg","ax2KCe"]},"Volume (%)":{"plot":["ax1Voli","ax1Volg","ax21/p.We0*100*We"]},"[Glu] (mM)":{"plot":["ax1GluCi","ax1GluCg","ax2GluCc"]},"size":[5,1]}'  --beta1 4 --beta2 4 

python set_up.py --solve --saveloc 'Finalfigures/AstrocytePresentExcitation' --readdata ''  --tfinal 7 --excite 1 6 25 0.9 --alphae0 0.2 --gltScale 0.5 --gltScaleAst 2 --pumpScaleNeuron 1.6 --pumpScaleAst 1.6 --plot '{"E$_m$ (mV)":{"plot":["ax1Vi","ax2Vg"],"ax1scale":[-80,10],"ax2scale":[-85,-20]},"[Na$^+$] (mM)":{"plot":["ax1NaCi","ax1NaCg","ax2NaCe"],"ax2scale":[140,160],"ax1scale":[5,16]},"[K$^+$] (mM)":{"plot":["ax1KCi","ax1KCg","ax2KCe"],"ax2scale":[1,12]},"Volume (%)":{"plot":["ax1Voli","ax1Volg","ax21/p.We0*100*We"],"ax1scale":[95,105],"ax2scale":[95,105]},"[Glu] (mM)":{"plot":["ax1GluCi","ax1GluCg","ax2GluCc"],"ax1scale":[0,3],"ax2scale":[1e-4,5e-4]},"size":[5,1]}' 

python set_up.py --solve --saveloc 'Finalfigures/AstrocyteAbsentExcitation' --readdata ''  --tfinal 7 --excite 1 6 25 0.9 --astblock 1 6 --alphae0 0.2 --gltScale 0.5 --gltScaleAst 2 --pumpScaleNeuron 1.6 --pumpScaleAst 1.6 --plot '{"E$_m$ (mV)":{"plot":["ax1Vi","ax2Vg"]},"[Na$^+$] (mM)":{"plot":["ax1NaCi","ax1NaCg","ax2NaCe"],"ax2scale":[140,160]},"[K$^+$] (mM)":{"plot":["ax1KCi","ax1KCg","ax2KCe"],"ax2scale":[1,12]},"Volume (%)":{"plot":["ax1Voli","ax1Volg","ax21/p.We0*100*We"],"ax1scale":[100,105],"ax2scale":[95,100]},"[Glu] (mM)":{"plot":["ax1GluCi","ax1GluCg","ax2GluCc"]},"size":[5,1]}' 

python set_up.py --solve --saveloc  'Finalfigures/VolumeBig'  --readdata '' --alphae0 0.8  --tstart 20 --tend 25 --t0 15 --tfinal 55 --perc 0.5 --gltScale 0.5 --gltScaleAst 2 --pumpScaleNeuron 1.6 --pumpScaleAst 1.6 --plot '{"E$_m$ (mV)":{"plot":["ax1Vi","ax2Vg"]},"[Na$^+$] (mM)":{"plot":["ax1NaCi","ax1NaCg","ax2NaCe"]},"[K$^+$] (mM)":{"plot":["ax1KCi","ax1KCg","ax2KCe"]},"Volume (%)":{"plot":["ax1Voli","ax1Volg","ax21/p.We0*100*We"]},"[Glu] (mM)":{"plot":["ax1Glu Ci","ax1GluCg","ax2GluCc"]},"size":[5,1]}' --beta1 4 --beta2 4 

python set_up.py --solve --saveloc  'Finalfigures/VolumeSmall' --readdata '' --alphae0 0.2  --tstart 20 --tend 25 --t0 15 --tfinal 55 --perc 0.5 --gltScale 0.5 --gltScaleAst 2 --pumpScaleNeuron 1.6 --pumpScaleAst 1.6 --plot '{"E$_m$ (mV)":{"plot":["ax1Vi","ax2Vg"]},"[Na$^+$] (mM)":{"plot":["ax1NaCi","ax1NaCg","ax2NaCe"]},"[K$^+$] (mM)":{"plot":["ax1KCi","ax1KCg","ax2KCe"]},"Volume (%)":{"plot":["ax1Voli","ax1Volg","ax21/p.We0*100*We"]},"[Glu] (mM)":{"plot":["ax1GluCi","ax1GluCg","ax2GluCc"]},"size":[5,1]}' --beta1 4 --beta2 4 

